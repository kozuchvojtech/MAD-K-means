      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KMeansProcessor"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1800"
      Title="DistributionResult">
    <Grid x:Name="PlotsGrid">
        <WpfPlot Name="CumulativeDistribution" Margin="0,500,900,0" />
        <WpfPlot Name="CumulativeDistributionEmpirical" Margin="900,500,0,0" />
    </Grid>
</Page>
