<Page x:Class="KMeansProcessor.DistributionResult"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KMeansProcessor"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1500"
      Title="DistributionResult">
    <Grid>
        <WpfPlot Name="NormalDistributionX" Margin="0,0,750,500" />
        <WpfPlot Name="NormalDistributionY" Margin="750,0,0,500" />

        <WpfPlot Name="CumulativeDistribution" Margin="0,500,750,0" />
        <WpfPlot Name="CumulativeDistributionEmpirical" Margin="750,500,0,0" />
    </Grid>
</Page>
